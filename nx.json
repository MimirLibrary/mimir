{
  "extends": "@nrwl/workspace/presets/core.json",
  "npmScope": "mimir",
  "affected": {
    "defaultBase": "main"
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "tasksRunnerOptions": {
    "default": {
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"],
        "accessToken": "YjU5OGVkYzMtMGY5My00ZTEzLWI0ZjQtYjFkOGZmZTliN2QzfHJlYWQtd3JpdGU=",
        "runtimeCacheInputs": [
          "echo $NX_API_ROOT_URL"
        ]
      }
    }
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "@emotion/styled",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "@emotion/styled"
      },
      "library": {
        "style": "@emotion/styled",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "frontapp"
}
